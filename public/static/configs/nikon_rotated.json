{
    "output_quality": 100,
    "config_name": "Nikon Rotated",
    "border_size": 0,
    "background": [255,255,255],
    "extra_length": "15%",
    "camera_args_direction": "down",
    "original_time_format": "%Y:%m:%d %H:%M:%S",
    "target_time_format": "%Y-%m-%d %H:%M:%S",
    "labels": [
        {
            "label_name": "camera model label",
            "font_path": "/static/fonts/AlibabaPuHuiTi-3-75-SemiBold.ttf",
            "font_size": "3.5%",
            "content": "${CAMERA_MODEL}",
            "position_offset": ["22%","4%"],
            "font_color": [0,0,0]
        },
        {
            "label_name": "camera lens model",
            "font_path": "/static/fonts/AlibabaPuHuiTi-3-45-Light.ttf",
            "font_size": "1.8%",
            "content": "${CAMERA_LENS_MODEL}",
            "position_offset": ["22%","8%"],
            "font_color": [0,0,0]
        },
        {
            "label_name": "photo original datetime",
            "font_path": "/static/fonts/AlibabaPuHuiTi-3-45-Light.ttf",
            "font_size": "1.5%",
            "content": "${PHOTO_ORIGINAL_DATETIME}",
            "position_offset": ["22%","10%"],
            "font_color": [0,0,0]
        },

        {
            "label_name": "camera args",
            "font_path": "/static/fonts/AlibabaPuHuiTi-3-45-Light.ttf",
            "font_size": "2.2%",
            "content": "${FOCAL_LENGTH}mm f/${APERTURE} ${EXPOSURE_TIME} ISO${CAMERA_ISO}",
            "position_offset": ["55%","5%"],
            "font_color": [0,0,0]
        },

        {
            "label_name": "camera sentence",
            "font_path": "/static/fonts/AlibabaPuHuiTi-3-75-SemiBold.ttf",
            "font_size": "2.5%",
            "content": "尼康刀锐奶化，人像神器！",
            "position_offset": ["55%","9%"],
            "font_color": [0,0,0]
        }
    ],
    "photos": [
        {
            "path": "/static/assets/camera_logos/NIKON/logo.png",
            "position_offset": ["3%","3%"],
            "scale": "15%"
        }
    ]
}
